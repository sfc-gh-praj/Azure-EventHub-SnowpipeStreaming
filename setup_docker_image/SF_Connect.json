{
    "name":"snowflakesink",
    "config":{
    "connector.class":"com.snowflake.kafka.connector.SnowflakeSinkConnector",
    "tasks.max":"16",
    "topics":"vehicle_iot_data",
    "buffer.count.records":"1000",
    "buffer.flush.time":"2",
    "buffer.size.bytes":"2000000",
    "snowflake.url.name":"account1.snowflakecomputing.com:443",
    "snowflake.user.name":"ss_user",
    "snowflake.private.key":"key",
    "snowflake.database.name":"snowpipe_streaming",
    "snowflake.schema.name":"dev",
    "key.converter":"org.apache.kafka.connect.storage.StringConverter",
    "value.converter":"org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable":"false",
    "snowflake.role.name":"accountadmin",
    "snowflake.ingestion.method":"SNOWPIPE_STREAMING",
    "snowflake.enable.schematization":"TRUE"
    }
    }
    